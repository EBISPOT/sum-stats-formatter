{ 
   "outFilePrefix":"formatted_",
   "md5":"True",
   "fieldSeparator":"\t",
   "removeLinesStarting":"#",
   "splitColumns":[ 
      { 
         "field":"variant_id",
         "delimiter":":",
         "leftName":"chr",
         "rightName":"snp1"
      },
      { 
         "field":"chromosome",
         "delimiter":":",
         "leftName":"snp2",
         "rightName":"snp3"
      }
   ],
   "columnConfig":[ 
      { 
         "field":"chromosome",
         "find":"CHR|chr|-|_",
         "replace":"",
         "keep":"True"
      },
      { 
         "field":"variant_id",
         "find":"^((?!^rs[0-9]*$).)*$",
         "replace":"",
         "rename":"snippy",
         "keep":"True"
      },
      { 
         "field":"effect_allele",
         "keep":"True"
      },
      { 
         "field":"beta",
         "keep":"True"
      },
      { 
         "field":"pval",
         "keep":"False"
      }
   ]
}
