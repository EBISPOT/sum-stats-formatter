{ 
   "files":"testfile*.tsv.gz",
   "outFilePrefix":"formatted_",
   "concatenateFiles":"False",
   "md5":"True",
   "fieldSeparator":"\t",
   "removeLinesStarting":"#",
   "splitColumns":[ 
      { 
         "field":"variant_id",
         "delimiter":":",
         "leftName":"snp0",
         "rightName":"snp1"
      },
      { 
         "field":"chromosome",
         "delimiter":":",
         "leftName":"snp2",
         "rightName":"snp3"
      }
   ],
   "keepColumns":[ 
      "chr",
      "bp",
      "snp0",
      "pval",
      "beta",
      "eaf"
   ],
   "findAndReplaceValue":[ 
      { 
         "field":"chromosome",
         "find":"CHR|chr|-|_",
         "replace":""
      },
      { 
         "field":"variant_id",
         "find":"^((?!^rs[0-9]*$).)*$",
         "replace":""
      },
      { 
         "field":"snp0",
         "find":"^((?!^rs[0-9]*$).)*$",
         "replace":""
      }
   ],
   "headerRename":[ 
      { 
         "chr":"chromosome"
      },
      { 
         "snp0":"variant_id"
      },
      { 
         "bp":"base_pair_location"
      },
      { 
         "pval":"p_value"
      }
   ]
}
