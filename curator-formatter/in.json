{ 
   "files":"testfile*.tsv.gz",
   "outFilePrefix":"formatted_",
   "concatenateFiles":"False",
   "md5":"True",
   "fieldSeparator":"\t",
   "removeLinesStarting":"#",
   "splitColumns":[ 
      { 
         "field":"variant_id",
         "delimiter":":",
         "leftName":"chr",
         "rightName":"snp1"
      },
      { 
         "field":"chromosome",
         "delimiter":":",
         "leftName":"snp2",
         "rightName":"snp3"
      }
   ],
   "findAndReplaceValue":[ 
      { 
         "field":"chromosome",
         "find":"CHR|chr|-|_",
         "replace":""
      },
      { 
         "field":"variant_id",
         "find":"^((?!^rs[0-9]*$).)*$",
         "replace":""
      },
      { 
         "field":"chr",
         "find":"^((?!^rs[0-9]*$).)*$",
         "replace":""
      }
   ],
   "headerRename": { 
         "chr": "chrom",
         "snp0": "variant",
         "bp": "base_pair_location",
         "p_value":"pval",
         "p-value": "p_value"
      },
   "keepColumns":[ 
      "chromosome",
      "base_pair_location",
      "p_value",
      "effect_allele",
      "beta",
      "variant_id"
   ]
}
